{
	"name": "hedger/laravel-collection-interpolate",
	"description": "Linear and time-aware interpolation method for Laravel Collections",
	"type": "library",
	"require": {
		"illuminate/support": "^11 || ^10"
	},
	"require-dev": {
		"phpunit/phpunit": "^11 || ^10",
		"orchestra/testbench": "^9 || ^8"
	},
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Hedger\\LaravelCollectionInterpolate\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Hedger\\LaravelCollectionInterpolate\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "Nicolas Hedger",
			"email": "nicolas@hedger.ch"
		}
	],
	"minimum-stability": "stable",
	"laravel": {
		"providers": ["Hedger\\LaravelCollectionInterpolate\\ServiceProvider"]
	}
}
